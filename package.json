{
  "name": "ratecaster-app",
  "version": "1.0.0",
  "description": "RateCaster dApp review application with backend caching service.",
  "main": "backend-service.ts",
  "type": "module",
  "scripts": {
    "start:backend": "tsx backend-service.ts",
    "start:frontend": "vite",
    "start": "concurrently \"npm:start:backend\" \"npm:start:frontend\"",
    "dev:backend": "nodemon --watch './*.ts' --watch './RateCasterSDK/**/*.ts' --exec 'tsx' ./backend-service.ts",
    "dev:frontend": "vite",
    "dev": "concurrently \"npm:dev:backend\" \"npm:dev:frontend\"",
    "build:backend": "tsc -p tsconfig.backend.json",
    "build:frontend": "vite build",
    "preview": "vite preview"
  },
  "keywords": [
    "dapp",
    "reviews",
    "blockchain",
    "ethers",
    "react"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "@rainbow-me/rainbowkit": "^2.2.6",
    "@ratecaster/sdk": "^0.0.4",
    "@tanstack/react-query": "^5.80.6",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "ethers": "^6.14.1",
    "express": "^4.19.2",
    "ratecaster-sdk": "file:../",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "socket.io": "^4.7.5",
    "socket.io-client": "^4.8.1",
    "tailwindcss": "^3.4.14",
    "viem": "^2.31.0",
    "wagmi": "^2.15.6"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.17.50",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react": "^4.3.3",
    "autoprefixer": "^10.4.20",
    "concurrently": "^8.2.2",
    "nodemon": "^3.1.4",
    "postcss": "^8.4.47",
    "tsx": "^4.19.4",
    "typescript": "^5.5.4",
    "vite": "^5.4.9"
  }
}
